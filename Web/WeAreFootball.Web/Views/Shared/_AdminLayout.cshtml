@using WeAreFootball.Common
@using WeAreFootball.Data.Models
@using Microsoft.AspNetCore.Identity
@using WeAreFootball.Services.Data.Contracts
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
@inject IUsersService UsersService
@{ var userImage = this.UsersService.GetUserImage(this.User.Identity.Name); }

<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>srtdash - SEO Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/png" href="../assets/images/icon/favicon.ico">
    <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/themify-icons.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/metisMenu.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/owl.carousel.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/slicknav.min.css" asp-append-version="true" />
    <!-- amchart css -->
    <link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />

    <!-- Start datatable css -->
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.18/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.jqueryui.min.css">


    <!-- others css -->
    <link rel="stylesheet" href="~/css/typography.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/default-css.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/customStyles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true" />
    <!-- modernizr css -->
    <script src="~/js/vendor/modernizr-2.8.3.min.js" asp-append-version="true"></script>
</head>

<body class="body-bg">
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <!-- preloader area start -->
    <div id="preloader">
        <div class="loader"></div>
    </div>
    <!-- preloader area end -->
    <!-- main wrapper start -->
    <div class="horizontal-main-wrapper">
        <!-- main header area start -->
        <div class="mainheader-area">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-3">
                    </div>
                    <!-- profile info & task notification -->
                    <div class="col-md-9 clearfix text-right">
                        <div class="d-md-inline-block d-block mr-md-4">
                            <ul class="notification-area">
                                <li id="full-view"><i class="ti-fullscreen"></i></li>
                                <li id="full-view-exit"><i class="ti-zoom-out"></i></li>
                            </ul>
                        </div>
                        <div class="clearfix d-md-inline-block d-block">
                            <div class="user-profile m-0">
                                <img class="avatar user-thumb" src="@userImage" alt="avatar">
                                <h4 class="user-name dropdown-toggle" data-toggle="dropdown">@this.User.Identity.Name <i class="fa fa-angle-down"></i></h4>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Back to websie</a>
                                    <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Manage/Index">Go to user panel</a>
                                    <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Logout">Log Out</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- main header area end -->
        <!-- header area start -->
        <div class="header-area header-bottom">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-9  d-none d-lg-block">
                        <div class="horizontal-menu">
                            <nav>
                                <ul id="nav_menu">
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="ti-pie-chart"></i>
                                            <span>Users table</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a asp-area="Administration" asp-controller="Users" asp-action="GetAll">Check all users</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="fa fa-table"></i>
                                            <span>Leagues table</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a asp-area="Administration" asp-controller="Leagues" asp-action="GetAll">Check all leagues</a></li>
                                            <li><a asp-area="Administration" asp-controller="Leagues" asp-action="Create">Create new league</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="fa fa-table"></i>
                                            <span>Teams table</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a asp-area="Administration" asp-controller="Teams" asp-action="GetAll">Check all teams</a></li>
                                            <li><a asp-area="Administration" asp-controller="Teams" asp-action="Create">Create new team</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="fa fa-table"></i>
                                            <span>News table</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a asp-area="Administration" asp-controller="News" asp-action="GetAll">Check all news</a></li>
                                            <li><a asp-area="Administration" asp-controller="News" asp-action="Create">Create news</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="fa fa-table"></i>
                                            <span>Events table</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a asp-area="Administration" asp-controller="Events" asp-action="GetAll">Check all events</a></li>
                                            <li><a asp-area="Administration" asp-controller="Events" asp-action="Create">Create event</a></li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="javascript:void(0)">
                                            <i class="fa fa-table"></i>
                                            <span>Go to Hangfire</span>
                                        </a>
                                        <ul class="submenu">
                                            <li><a href="/Hangfire">Hangfire</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <!-- nav and search button -->
                    <div class="col-lg-3 clearfix">
                        <div class="search-box">
                        </div>
                    </div>
                    <!-- mobile_menu -->
                </div>
            </div>
        </div>
        <!-- header area end -->
        <!-- page title area end -->
        <div class="main-content-inner">
            <div class="container">
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>

        <!-- main content area end -->
        <!-- footer area start-->
        <footer>
            <div class="footer-area">
                <p>© Copyright 2018. All right reserved. Template by <a href="https://colorlib.com/wp/">Colorlib</a>.</p>
            </div>
        </footer>
        <!-- footer area end-->



    </div>


    <!-- jquery latest version -->
    <script src="~/js/vendor/jquery-2.2.4.min.js" asp-append-version="true"></script>
    <!-- bootstrap 4 js -->
    <script src="~/js/popper.min.js" asp-append-version="true"></script>
    <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/js/metisMenu.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.slimscroll.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.slicknav.min.js" asp-append-version="true"></script>

    <!-- start chart js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>
    <!-- start highcharts js -->
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
    <script src="https://code.highcharts.com/modules/export-data.js"></script>
    <!-- start amcharts -->
    <script src="https://www.amcharts.com/lib/3/amcharts.js"></script>
    <script src="https://www.amcharts.com/lib/3/ammap.js"></script>
    <script src="https://www.amcharts.com/lib/3/maps/js/worldLow.js"></script>
    <script src="https://www.amcharts.com/lib/3/serial.js"></script>
    <script src="https://www.amcharts.com/lib/3/plugins/export/export.min.js"></script>
    <script src="https://www.amcharts.com/lib/3/themes/light.js"></script>



    <!-- Start datatable js -->
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>
    <script src="https://cdn.datatables.net/1.10.18/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.18/js/dataTables.bootstrap4.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.3/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.3/js/responsive.bootstrap.min.js"></script>





    <!-- all line chart activation -->
    <script src="~/js/line-chart.js" asp-append-version="true"></script>
    <!-- all pie chart -->
    <script src="~/js/pie-chart.js" asp-append-version="true"></script>
    <!-- all bar chart -->
    <script src="~/js/bar-chart.js" asp-append-version="true"></script>
    <!-- all map chart -->
    <script src="~/js/maps.js" asp-append-version="true"></script>
    <!-- others plugins -->
    <script src="~/js/plugins.js" asp-append-version="true"></script>
    <script src="~/js/scripts.js" asp-append-version="true"></script>


    @await this.RenderSectionAsync("Scripts", required: false)

</body>

</html>
